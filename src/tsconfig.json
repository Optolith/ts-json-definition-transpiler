{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "lib": ["ESNext"],
    "module": "NodeNext",
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "../lib",
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
  }
}
